{
  "compilerOptions": {
    "types": ["@trace/shared", "@trace/model"],
    "paths": {
      "@/*": ["./*"],
      "@@/*": ["./*"],
      "~/*": ["./*"],
      "~~/*": ["./*"]
    }
  },
  "extends": "./.playground/.nuxt/tsconfig.json",
  "vueCompilerOptions": {
    "target": 3,
    "plugins": [
      "@vue-macros/volar/define-models",
      "@vue-macros/volar/short-vmodel"
    ],
    "vueMacros": {
      "defineModels": {},
      "shortVmodel": {
        "prefix": "$"
      }
    }
  }
}
