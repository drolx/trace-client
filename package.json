{
  "name": "@trace/client",
  "version": "0.0.1-preview.1",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/drolx/trace-client.git"
  },
  "keywords": [
    "gps",
    "track"
  ],
  "author": "radioActive DROID <me@godwin.dev>",
  "license": "BSL-1.1",
  "bugs": {
    "url": "https://github.com/drolx/trace-client/issues"
  },
  "homepage": "https://trace.ng",
  "scripts": {
    "clean": "pnpx rimraf node_modules **/node_modules bun.* **/bun.* **/dist/* pnpm-lock.* **/pnpm-lock.*",
    "dev": "pnpm -r --filter ./apps run dev",
    "dev:base": "pnpm -r --filter ./base run dev",
    "dev:manager": "pnpm -r --filter ./manager run dev",
    "dev:website": "pnpm -r --filter ./website run dev",
    "dev:docs": "pnpm -r --filter ./docs run dev",
    "build": "pnpm -r --filter ./apps run build",
    "translate": "tsx ./translate.ts",
    "translate:lint": "eslint --fix --ext .json --format node_modules/eslint-plugin-i18n-json/formatter.js locales/"
  },
  "devDependencies": {
    "@parvineyvazov/json-translator": "^1.8.0",
    "eslint": "^8.0.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "pnpm": "^8.2.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.13.0",
    "typescript": "^5.0.0",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "packages/*",
    "base",
    "website",
    "docs",
    "apps/*"
  ],
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">= 8.2.0",
    "yarn": "please-use-pnpm",
    "bun": ">= 1.0.1",
    "node": ">= 16.0.0"
  }
}
