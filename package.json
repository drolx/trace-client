{
  "name": "@trace/client",
  "version": "0.0.1-alpha.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/drolx/trace-client.git"
  },
  "keywords": [
    "gps",
    "track"
  ],
  "author": "radioActive DROID <me@godwin.dev>",
  "license": "BSL-1.1",
  "bugs": {
    "url": "https://github.com/drolx/trace-client/issues"
  },
  "homepage": "https://trace.ng",
  "scripts": {
    "clean": "pnpx rimraf node_modules **/node_modules bun.* **/bun.* **/dist/* pnpm-lock.* **/pnpm-lock.*",
    "dev": "pnpm -r --filter ./apps run dev",
    "dev:app": "pnpm -r --filter ./base-app run dev",
    "dev:support": "pnpm -r --filter ./apps/support run dev",
    "dev:portal": "pnpm -r --filter ./apps/portal run dev",
    "dev:finder": "pnpm -r --filter ./apps/finder run dev",
    "dev:core": "pnpm -r --filter ./apps/core run dev",
    "dev:manager": "pnpm -r --filter ./apps/manager run dev",
    "build": "pnpm -r --filter ./apps run build"
  },
  "devDependencies": {
    "pnpm": "^8.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.0",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "packages/*",
    "base-app",
    "apps/*"
  ],
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": ">= 8.2.0",
    "yarn": "please-use-pnpm",
    "bun": ">= 1.0.1",
    "node": ">= 16.0.0"
  }
}